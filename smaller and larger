//User function template for C++
class Solution{
public:	
	vector<int> getMoreAndLess(int arr[], int n, int x) {
	    // code here
	    

	    int less=0;
	    int more=0;
	    
	    for(int i=0;i<n;i++)
	    {
	        if(arr[i]<=x)
	        {
	            less+=1;
	        }
	        if(arr[i]>=x)
	        {
	            more+=1;
	        }
	    }
	    return {less,more};
	    
	    
	    
	    
	    
	    
	    
	}
};
